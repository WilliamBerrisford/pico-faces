{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "${defaultBuildTask}",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe_rs Launch",
            "flashingConfig": {
                "flashingEnabled": true,
            },
            "chip": "RP2040",
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "rttEnabled": true,
                    "programBinary": "${workspaceRoot}/target/thumbv6m-none-eabi/debug/pico-w-birthday"
                }
            ],
            "env": {
                "DEFMT_LOG": "info",
            },
            "consoleLogLevel": "Console"
        },
        {
            "type": "probe-rs-debug",
            "request": "attach",
            "name": "probe_rs Attach",
            "chip": "RP2040",
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "rttEnabled": true,
                    "programBinary": "${workspaceRoot}/target/thumbv6m-none-eabi/debug/pico-w-birthday"
                }
            ],
            "env": {
                "DEFMT_LOG": "info",
            },
            "consoleLogLevel": "Console"
        }
    ]
}